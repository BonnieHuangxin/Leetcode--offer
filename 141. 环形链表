# Definition for singly-linked list.
# class ListNode(object):
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution(object):
    def hasCycle(self, head):
        """
        :type head: ListNode
        :rtype: bool
        """
        #ä½¿ç”¨å¿«æ…¢æŒ‡é’ˆï¼Œè‹¥æŒ‡é’ˆç›¸é‡åˆ™åˆ¤æ–­æœ‰ç¯
        #å¿«æŒ‡é’ˆé€Ÿåº¦æ˜¯æ…¢æŒ‡é’ˆçš„ä¸¤å€ã€‚ç»ˆæ­¢æ¡ä»¶æ˜¯å¿«æŒ‡é’ˆæŒ‡å‘Noneæˆ–è€…å¿«æ…¢æŒ‡é’ˆç›¸ç­‰ã€‚æƒ³è±¡æˆä¸¤åŒ¹ğŸåœ¨ç¯å½¢è·‘é“ä¸Šè¿½é€ï¼Œåªè¦é€Ÿåº¦ä¸ä¸€æ ·ï¼Œè¿Ÿæ—©ç›¸é‡
        #p:æ…¢æŒ‡é’ˆ q:å¿«æŒ‡é’ˆ
        if head == None:
            return False
        p = head
        q = head
        while q:
            if q.next == None:
                return False
            p = p.next
            q = q.next.next
            
            if p == q:
                return True
        return False
